package smp;
class COPY
{
	int Rollnumber;
	String Name;
	COPY(int Rollnumber,String Name)
	{
		
		this.Rollnumber=Rollnumber;
		this.Name=Name;
	}
	
	COPY(COPY O2)
	{
		
		this.Rollnumber=O2.Rollnumber;
		this.Name=O2.Name;
	}
	


}
public class COPYCon {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
COPY O1=new COPY(1,"qa");
System.out.println("Original Constructor"+O1.Name+" "+O1.Rollnumber);
COPY O2=new COPY(O1);
System.out.println("Copy Constructor "+O2.Name+" "+O2.Rollnumber);
	}

}
